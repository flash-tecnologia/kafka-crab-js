{
  "name": "kafka-crab-js-integration-tests",
  "version": "1.0.0",
  "type": "module",
  "description": "Integration tests for kafka-crab-js",
  "scripts": {
    "test": "./run-tests.sh",
    "test:manual": "node --test *.test.mjs",
    "test:producer": "node producer.test.mjs",
    "test:consumer": "node consumer.test.mjs",
    "test:stream": "node consumer-stream.test.mjs",
    "test:batch": "node consumer-stream-batch.test.mjs",
    "kafka:up": "docker-compose up -d || podman-compose up -d",
    "kafka:down": "docker-compose down -v || podman-compose down -v",
    "kafka:logs": "docker-compose logs -f kafka || podman-compose logs -f kafka",
    "kafka:ui": "docker-compose --profile ui up -d || podman-compose --profile ui up -d",
    "kafka:up:podman": "podman-compose up -d",
    "kafka:down:podman": "podman-compose down -v",
    "kafka:logs:podman": "podman-compose logs -f kafka",
    "kafka:ui:podman": "podman-compose --profile ui up -d"
  },
  "dependencies": {
    "nanoid": "^5.0.0"
  }
}